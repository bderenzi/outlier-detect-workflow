<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
  <h:head>
    <h:title>outlier_detection_form</h:title>
    <model>
      <instance>
        <data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/9993B6A9-29E1-43F2-88B3-369DB14D8DA7" uiVersion="1" version="1" name="outlier_detection_form">
          <username/>
          <questionid/>
          <score/>
          <p_value/>
          <observed_freq/>
          <expected_freq/>
          <outliermonth/>
        </data>
      </instance>
      <instance src="jr://instance/session" id="commcaresession"/>
      <bind vellum:nodeset="#form/username" nodeset="/data/username" calculate="instance('commcaresession')/session/context/username"/>
      <bind vellum:nodeset="#form/questionid" nodeset="/data/questionid" type="xsd:string"/>
      <bind vellum:nodeset="#form/score" nodeset="/data/score" type="xsd:double"/>
      <bind vellum:nodeset="#form/p_value" nodeset="/data/p_value" type="xsd:double"/>
      <bind vellum:nodeset="#form/observed_freq" nodeset="/data/observed_freq" type="xsd:string"/>
      <bind vellum:nodeset="#form/expected_freq" nodeset="/data/expected_freq" type="xsd:string"/>
      <bind vellum:nodeset="#form/outliermonth" nodeset="/data/outliermonth" type="xsd:date"/>
      <itext>
        <translation lang="en" default="">
          <text id="questionid-label">
            <value>questionid</value>
          </text>
          <text id="score-label">
            <value>score</value>
          </text>
          <text id="p_value-label">
            <value>p_value</value>
          </text>
          <text id="observed_freq-label">
            <value>observed_freq</value>
          </text>
          <text id="expected_freq-label">
            <value>expected_freq</value>
          </text>
          <text id="outliermonth-label">
            <value>outliermonth</value>
          </text>
        </translation>
      </itext>
    </model>
  </h:head>
  <h:body>
    <input vellum:ref="#form/questionid" ref="/data/questionid">
      <label ref="jr:itext('questionid-label')"/>
    </input>
    <input vellum:ref="#form/score" ref="/data/score">
      <label ref="jr:itext('score-label')"/>
    </input>
    <input vellum:ref="#form/p_value" ref="/data/p_value">
      <label ref="jr:itext('p_value-label')"/>
    </input>
    <input vellum:ref="#form/observed_freq" ref="/data/observed_freq">
      <label ref="jr:itext('observed_freq-label')"/>
    </input>
    <input vellum:ref="#form/expected_freq" ref="/data/expected_freq">
      <label ref="jr:itext('expected_freq-label')"/>
    </input>
    <input vellum:ref="#form/outliermonth" ref="/data/outliermonth">
      <label ref="jr:itext('outliermonth-label')"/>
    </input>
  </h:body>
</h:html>